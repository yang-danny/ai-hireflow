# Environment variables for performance monitoring

# Sentry Configuration
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_ENABLE_SENTRY=false  # Set to 'true' to enable Sentry in development
VITE_APP_VERSION=1.0.0

